{
  "name": "sls-express",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "clear": "rmdir /s \"dist\"",
    "build": "tsc",
    "start": "npm run build && node .",
    "watch": "nodemon",
    "deploy": "sls deploy --verbose",
    "deploy:prod": "cross-env-shell AWS_STAGE=prod STAGE_TYPE=prod sls deploy --verbose",
    "undeploy": "sls remove --verbose",
    "offline": "sls offline start",
    "test": "npm run unit && npm run acceptance",
    "unit": "jest --group=unit",
    "acceptance": "jest --group=acceptance --maxWorkers=1",
    "openapi": "ts-node openapi/generate-openapi.ts openapi/dist",
    "sdk": "npm run openapi && npx @openapitools/openapi-generator-cli generate -i openapi/dist/openapi.json -g typescript-node -o openapi/dist/api-sdk -c openapi/config.json"
  },
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1282.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "pino": "^8.6.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@serverless/typescript": "^3.21.0",
    "@types/aws-lambda": "^8.10.106",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.7.20",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "aws-sdk-mock": "^5.8.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.9",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.24.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "jest": "^29.3.1",
    "jest-junit": "^15.0.0",
    "jest-runner-groups": "^2.2.0",
    "json-schema-to-ts": "^2.6.2",
    "nodemon": "^2.0.20",
    "serverless": "^3.22.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-esbuild": "^1.33.0",
    "serverless-offline": "^10.2.1",
    "serverless-plugin-typescript": "^2.1.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.8.3"
  }
}
